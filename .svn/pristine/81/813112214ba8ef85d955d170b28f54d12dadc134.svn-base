.carousel {
  height: 500px;
}
.carousel .carousel-indicators {
  position: absolute;
  bottom: 15px;
  top: inherit;
  right: 50%;
  z-index: 5;
  margin: 0;
  list-style: none;
}
.carousel .carousel-inner .item {
  width: 100%;
  height: 500px;
}
.carousel img {
  min-height: 500px;
  width: 100%;
}
.carousel .carousel-control {
  margin-top: 0px;
  border: none;
  opacity: 0.5;
  font-family: "futuraicg";
  background: none;
}
.top {
  position: relative;
  width: 100%;
  z-index: 10;
  height: 74px;
  background-color: rgba(255, 255, 255, 0.5);
}
.top .container {
  overflow: hidden;
}
.top .container .logo {
  float: left;
  padding-top: 20px;
  padding-right: 30px;
}
.top .container .logo img {
  width: 200px;
  height: 40px;
}
.top .container .top_nav {
  float: left;
  width: 60%;
}
.top .container .top_nav ul.nav {
  margin-bottom: 0px;
}
.top .container .top_nav ul.nav > li > a {
  padding: 30px 20px;
  margin: 0;
}
.top .container .top_nav ul.nav > li:hover > ul.dropdown-menu {
  display: block;
  margin-top: 0px;
}
.top .container .top_nav ul.nav ul.dropdown-menu {
  -moz-border-radius: 0px;
  -webkit-border-radius: 0px;
  -o-border-radius: 0px;
  border-radius: 0px;
  padding: 0px;
  margin-top: 0px;
  border: none;
}
.top .container .top_nav ul.nav ul.dropdown-menu li a {
  background-color: #008000;
  padding: 10px 10px;
  background-image: none;
  margin: 0;
}
.top .container .top_nav ul.nav ul.dropdown-menu li a:hover {
  background-color: rgba(0, 128, 0, 0.8);
}
.top .container .top_nav ul.nav > li:hover > a {
  background-color: #008000;
}
.top .container .top_nav ul.nav > li.active a {
  background-color: #008000;
}
.top .container .top_nav ul.nav a {
  color: #ffffff;
  -moz-border-radius: 0px;
  -webkit-border-radius: 0px;
  -o-border-radius: 0px;
  border-radius: 0px;
}
.search {
  float: left;
  overflow: hidden;
  width: 140px;
  padding: 20px 0;
}
.search .input {
  float: left;
  width: 80%;
}
.search .input input {
  width: 109%;
}
.search .bt {
  float: right;
  width: 20%;
}
.search .bt input {
  margin-top: 5px;
}
.about {
  padding: 50px 0;
  border-bottom: 1px solid #999999;
}
.about .content {
  padding: 10px 0px;
}
.about .content p {
  text-indent: 30px;
}
.lingshan {
  padding: 50px 0;
  border-bottom: 1px solid #999999;
}
.lingshan .content .cat-nav {
  overflow: hidden;
  margin: 10px auto;
  width: 200px;
}
.lingshan .content .cat-nav > div {
  float: left;
  padding: 10px 20px;
}
.lingshan .content .cat-nav .cat-name {
  border: 1px solid #008000;
  background-color: #e9e8e4;
  color: #333333;
  cursor: pointer;
}
.lingshan .content .cat-nav .active {
  background-color: #008000;
  color: #fefefe;
}
.lingshan .content .summary {
  padding-top: 20px;
}
.lingshan .content .summary .wrap {
  overflow: hidden;
}
.lingshan .content .summary .wrap .page {
  width: 360px;
  height: 470px;
  float: left;
  padding-top: 5px;
  background-color: white;
  color: #333333;
  margin: 10px;
  text-align: center;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  -o-border-radius: 10px;
  border-radius: 10px;
}
.lingshan .content .summary .wrap .page .thumb {
  width: 320px;
  margin: 10px auto;
}
.lingshan .content .summary .wrap .page .thumb img {
  width: 100%;
  height: 220px;
}
.lingshan .content .summary .wrap .page .brief {
  padding: 10px 30px;
  text-indent: 20px;
}
.lingshan .content .summary .wrap .dg-center {
  background-color: #008000;
  color: #fefefe;
}
.lingshan .content .summary .wrap .dg-center .more a {
  color: #fefefe;
  border: 1px solid #fefefe;
}
.lingshan .content .summary .wrap .brief {
  margin: 20px 0;
  text-align: left;
}
.lingshan .content .summary > div:not(:first-child) {
  display: none;
}
.news {
  padding: 50px 0;
  border-bottom: 1px solid #999999;
}
.content-block {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: stretch;
}
.content-block a {
  color: #333333;
}
.content-block a .block {
  width: 280px;
  background-color: #fefefe;
  margin: 10px 1px;
  text-align: center;
}
.content-block a .block .thumb {
  width: 100%;
  margin-bottom: 10px;
}
.content-block a .block .thumb img {
  width: 100%;
  height: 190px;
}
.content-block a .block .title,
.content-block a .block .date,
.content-block a .block .brief {
  padding: 5px 20px;
}
.content-block a .block .title {
  font-size: 14px;
  height: 45px;
  text-align: left;
}
.content-block a .block .date {
  text-align: right;
}
.content-block a .block .brief {
  height: 80px;
  text-indent: 20px;
  text-align: left;
}
.donate {
  padding: 50px 0;
  border-bottom: 1px solid #999999;
}
.donate .content {
  width: 1000px;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: stretch;
}
.donate .content .item {
  height: 300px;
  width: 300px;
  background-image: url("../images/wrap.png");
}
.donate .content .item .title {
  margin-top: 30px;
}
.donate .content .disclosure .details {
  padding: 30px;
  text-align: center;
}
.donate .content .disclosure .details strong {
  font-size: 40px;
  color: #008000;
}
.donate .content .disclosure .details p {
  margin: 20px 0;
}
.donate .content .donate-list .details {
  padding: 10px 30px;
}
.donate .content .support .details {
  padding-top: 40px;
}
.donate .content .support .details .one {
  width: 160px;
  margin: 10px auto;
}
.donate .content .support .details .one img {
  width: 40px;
}
.donate .content .support .details .one a {
  width: 70px;
  margin-left: 10px;
  border: none;
  text-align: center;
  background-color: #008000;
  color: #fefefe;
  background-image: none;
  text-shadow: none;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  -o-border-radius: 10px;
  border-radius: 10px;
}
.extra {
  padding: 50px 0;
}
.extra .msg {
  text-align: center;
  font-weight: inherit;
}
.extra input.btn {
  height: 40px;
  background-color: #008000;
  background-image: none;
  color: #fefefe;
  border: none;
  text-shadow: none;
  border-bottom-left-radius: 0px;
  border-top-left-radius: 0px;
  -o-border-bottom-left-radius: 0px;
  -o-border-top-left-radius: 0px;
  -webkit-border-bottom-left-radius: 0px;
  -webkit-border-top-left-radius: 0px;
  -moz-border-bottom-left-radius: 0px;
  -moz-border-top-left-radius: 0px;
  border-bottom-right-radius: 10px;
  border-top-right-radius: 10px;
  -o-border-bottom-right-radius: 10px;
  -o-border-top-right-radius: 10px;
  -webkit-border-bottom-right-radius: 10px;
  -webkit-border-top-right-radius: 10px;
  -moz-border-bottom-right-radius: 10px;
  -moz-border-top-right-radius: 10px;
}
.extra .subscribe {
  padding: 20px 0 0 0;
}
.extra .subscribe form {
  width: 280px;
  margin: 0 auto;
}
.extra .subscribe form input[type="text"] {
  height: 40px;
  padding: 0;
  margin: 0;
  border: none;
  padding-left: 10px;
  margin-right: -10px;
  border-bottom-left-radius: 10px;
  border-top-left-radius: 10px;
  -o-border-bottom-left-radius: 10px;
  -o-border-top-left-radius: 10px;
  -webkit-border-bottom-left-radius: 10px;
  -webkit-border-top-left-radius: 10px;
  -moz-border-bottom-left-radius: 10px;
  -moz-border-top-left-radius: 10px;
  border-bottom-right-radius: 0px;
  border-top-right-radius: 0px;
  -o-border-bottom-right-radius: 0px;
  -o-border-top-right-radius: 0px;
  -webkit-border-bottom-right-radius: 0px;
  -webkit-border-top-right-radius: 0px;
  -moz-border-bottom-right-radius: 0px;
  -moz-border-top-right-radius: 0px;
}
.extra .download {
  width: 400px;
  text-align: center;
  padding: 20px 0 0 0;
  margin: 0 auto;
}
.extra .download input.btn {
  border-bottom-left-radius: 10px;
  border-top-left-radius: 10px;
  -o-border-bottom-left-radius: 10px;
  -o-border-top-left-radius: 10px;
  -webkit-border-bottom-left-radius: 10px;
  -webkit-border-top-left-radius: 10px;
  -moz-border-bottom-left-radius: 10px;
  -moz-border-top-left-radius: 10px;
  border-bottom-right-radius: 10px;
  border-top-right-radius: 10px;
  -o-border-bottom-right-radius: 10px;
  -o-border-top-right-radius: 10px;
  -webkit-border-bottom-right-radius: 10px;
  -webkit-border-top-right-radius: 10px;
  -moz-border-bottom-right-radius: 10px;
  -moz-border-top-right-radius: 10px;
}
.navigation ul {
  height: 32px;
  border-left: 1px solid #d7d7d7;
  width: 100%;
  margin: 0;
}
.navigation ul li {
  height: 31px;
  line-height: 31px;
  float: left;
  border-top: 1px solid #008000;
  border-bottom: 1px solid #008000;
  border-left: none;
  margin-bottom: -1px;
  background: #008000;
  overflow: hidden;
  position: relative;
}
.navigation ul li a {
  display: block;
  padding: 0 20px;
  outline: none;
  color: #fefefe;
  text-decoration: none;
}
.navigation ul li.active {
  background: #fff;
  border: 1px solid #008000;
  border-bottom: 1px solid #fff;
}
.navigation ul li.active a {
  color: #008000;
}
.single,
.linelist,
.article,
.blocklist,
.search-res {
  padding-bottom: 74px;
}
.single .banner,
.linelist .banner,
.article .banner,
.blocklist .banner,
.search-res .banner {
  width: 100%;
  height: 400px;
  background-size: cover;
  background-repeat: no-repeat;
  margin-bottom: 30px;
}
.single .content,
.linelist .content,
.article .content,
.blocklist .content,
.search-res .content {
  background-color: #FFF;
  border: 1px solid #d7d7d7;
  padding: 30px 40px;
}
.linelist .list-box ul {
  list-style: none;
  margin: 0;
}
.linelist .list-box ul li {
  padding: 5px 0;
}
.linelist .list-box ul li a {
  color: #008000;
}
.linelist .list-box .pull-right {
  text-align: right;
}
.blocklist .content-block {
  overflow: hidden;
  display: block;
}
.blocklist .content-block .block {
  float: left;
  border: 1px solid #E8E8E8;
  width: 250px;
  margin: 17px;
}
.search-res .listfull {
  background-color: #FFF;
  padding: 30px;
}
.search-res .list-content {
  padding: 20px 0;
}
.search-res .list-content .date {
  float: right;
  font-size: 14px;
}
.search-res .list-content i {
  margin: 4px 0 0 5px;
}
.label-tag {
  margin: 0 5px;
  background-color: #BEBEBE;
}
.label-select {
  background-color: #008000;
}
.article .article-infos {
  overflow: hidden;
  padding: 15px 0;
}
.article .article-infos .article-info {
  float: left;
  margin: 0 10px;
}
.article .article-infos .pull-right {
  float: right;
}
.article .summary {
  padding: 5px 0;
}
.article .details {
  padding: 20px 0;
}
.article .related {
  border: 1px solid #d7d7d7;
  border-top: none;
  background-color: #FFF;
  padding: 10px;
}
.article .related .title {
  font-size: 20px;
  font-weight: inherit;
}
.article .related ul {
  list-style: none;
  overflow: hidden;
  padding: 10px 0;
}
.article .related ul li {
  width: 50%;
  float: left;
  padding: 5px 0;
}
.article i {
  margin-top: 4px;
  margin-right: 5px;
}
.breadcrumb a {
  color: #008000;
}
#footer {
  background-color: #008000;
  padding: 50px 0;
}
#footer .container {
  color: #fefefe;
  text-align: center;
}
#footer .container .logo {
  text-align: center;
}
#footer .container p {
  margin: 10px 0;
}
#footer .container a {
  color: #fefefe;
  text-decoration: none;
}
.title {
  text-align: center;
  font-size: 20px;
  padding: 10px 0;
}
.title .eng {
  font-family: "futuraicg";
  font-weight: bold;
}
.title .mid {
  margin: 0 10px;
}
.content {
  padding: 10px 0;
}
.more {
  width: 100px;
  margin: 10px auto;
}
.more a {
  color: #fefefe;
  background-color: #008000;
  padding: 5px 10px;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  -o-border-radius: 10px;
  border-radius: 10px;
}
.margin-nav {
  margin-top: -74px;
}
.padding-nav {
  padding-top: 74px;
}
a,
a:hover {
  text-decoration: none;
}
body {
  font-family: "Microsoft Yahei";
  background-color: #e9e8e4;
  position: relative;
}
p span {
  background-color: rgba(255, 255, 255, 0) !important;
}
@font-face {
  font-family: 'futuraicg';
  src: url('../font/futuraicg-light/futuraicg-light-webfont.eot');
