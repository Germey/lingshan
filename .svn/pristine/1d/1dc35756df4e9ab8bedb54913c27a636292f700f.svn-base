@media (min-width: 1367px) {
  .carousel {
    height: 500px;
  }
  .carousel img {
    height: 500px !important;
  }
  .carousel .carousel-inner .item {
    height: 500px;
  }
  .single .banner,
  .linelist .banner,
  .article .banner,
  .blocklist .banner,
  .search-res .banner {
    height: 400px;
    margin-bottom: 0;
  }
}
@media (max-width: 1366px) and (min-width: 900px) {
  .carousel {
    height: 400px;
  }
  .carousel img {
    height: 400px !important;
  }
  .carousel .carousel-inner .item {
    height: 400px;
  }
  .single .banner,
  .linelist .banner,
  .article .banner,
  .blocklist .banner,
  .search-res .banner {
    height: 300px;
    margin-bottom: 0;
  }
}
@media (max-width: 899px) {
  .carousel {
    height: 300px;
  }
  .carousel img {
    height: 300px !important;
  }
  .carousel .carousel-inner .item {
    height: 300px;
  }
  .single .banner,
  .linelist .banner,
  .article .banner,
  .blocklist .banner,
  .search-res .banner {
    height: 260px;
    margin-bottom: 0;
  }
  .top .container {
    width: 100%;
  }
}
@media (max-width: 992px) and (min-width: 768px) {
  .content-block a .block {
    width: 230px;
  }
  .content-block a .block .thumb img {
    height: 160px;
  }
}
